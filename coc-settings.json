{
    "explorer.icon.enableNerdfont": true,
    "explorer.file.showHiddenFiles": true,
    "explorer.keyMappings.global": {
        "<space>": "toggleSelection",
        "v": "open:vsplit:intelligent"
    },
    "diagnostic.errorSign": "",
    "diagnostic.hintSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.warningSign": "",
    "clangd.semanticHighlighting": true,
    "clangd.arguments": ["-cross-file-rename"],
    "clangd.fallbackFlags": ["-std=c++2a"],
    "coc.preferences.formatOnSaveFiletypes": ["cpp", "python", "cmake", "rust"],
    "python.formatting.provider": "yapf",
    "python.formatting.yapfPath": "~/.local/bin/yapf",
    "suggest.noselect": false,
    "snippets.ultisnips.directories": [
        "UltiSnips",
        "~/.config/nvim/snippets"
    ],
    "list.source.files.args": ["--hidden", "--files"],
    "session.saveOnVimLeave": false,
    "git.addGBlameToVirtualText": true,
    "markdown-preview-enhanced.usePandocParser": true,
    "texlab.build.args": [
        "-xelatex",
        "-interaction=nonstopmode",
        "-synctex=1",
        "%f"
    ],
    "texlab.build.onSave": false,
    "texlab.build.forwardSearchAfter": false,
    "texlab.forwardSearch.executable": "zathura",
    "texlab.forwardSearch.args": ["--synctex-forward", "%l:1:%f", "%p"],
    "yank.enableCompletion": false,
    "outline.sortBy": "position",
    "outline.expandLevel": 0,
    "cSpell.userWords": [
      "Abaqus",
      "Biot",
      "Biot's",
      "Eikonal",
      "LEFM",
      "Qing",
      "Raphson",
      "Sneddon",
      "Terzaghi",
      "Terzaghi's",
      "cmake",
      "conda",
      "discretization",
      "discretize",
      "discretized",
      "dofs",
      "dtype",
      "elastoplastic",
      "geomaterials",
      "geomechanical",
      "linearization",
      "matplotlib",
      "numpy",
      "pcolormesh",
      "poroelastic",
      "poroelasticity",
      "poromechanical",
      "pybind",
      "pyproject",
      "uncracked",
      "vectorize",
      "wellbore",
      "xticks",
      "yticks"
    ],
    "cSpell.enabledLanguageIds": [
        "vim",
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "tex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "python",
        "pug",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "cSpell.ignoreRegExpList": [
      "\\\\\\w*(\\[.*?\\])?(\\{.*?\\})?",
      "\\$.+?\\$"
    ]
}
